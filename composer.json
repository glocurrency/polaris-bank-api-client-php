{
    "name": "glocurrency/polaris-bank-api-client-php",
    "description": "Polaris Bank API Client for PHP",
    "type": "library",
    "license": "MPL-2.0",
    "autoload": {
        "psr-4": {
            "Glocurrency\\PolarisBank\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Glocurrency\\PolarisBank\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "che dilas yusuph",
            "email": "josephdilas@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "*",
        "brokeyourbike/http-client": "^1.0",
        "brokeyourbike/resolve-uri": "^1.0",
        "brokeyourbike/http-enums": "^2.0",
        "brokeyourbike/has-source-model": "^2.0",
        "psr/simple-cache": "^3.0",
        "brokeyourbike/data-transfer-object": "^0.2.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.5",
        "nesbot/carbon": "^2.66"
    },
    "version": "0.0.1",
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    }
    
}
